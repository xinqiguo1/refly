    "dotenv": "^16.4.0",
    "langchain": "0.3.30",
    "uuid": "^10.0.0",
    "zod": "^3.23.0",
    "@scalebox/sdk": "^3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.15.0",
    "typescript": "^5.4.0"
